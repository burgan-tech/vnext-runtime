# VNext Execution Environment Template

# Application Settings
APP_NAME=vnext-execution-app-discovery
APP_DOMAIN=discovery
APP_PORT=5000
APP_HOST=0.0.0.0

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@vnext-postgres:5432/vNext_Discovery
DATABASE_HOST=vnext-postgres
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres
DATABASE_NAME=vNext_Discovery

# Redis Configuration
REDIS_HOST=vnext-redis
REDIS_PORT=6379

# Vault Configuration
VAULT_URL=http://vnext-vault:8200
VAULT_TOKEN=admin

# Dapr Configuration
DAPR_HTTP_PORT=43610
DAPR_GRPC_PORT=43611

# Logging
LOG_LEVEL=info

# Environment
ASPNETCORE_ENVIRONMENT=Development

# Dapr Store Names (REQUIRED)
DAPR_PLACEMENT_HOST=dapr-placement:50005
DAPR_APP_ID=vnext-execution-app-discovery
DAPR_SECRET_STORE_NAME=vnext-secret
DAPR_STATE_STORE_NAME=vnext-state
DAPR_PUBSUB_STORE_NAME=vnext-pubsub
DAPR_PUBSUB_BROADCAST_STORE_NAME=vnext-pubsub-broadcast
DAPR_LOCK_STORE_NAME=vnext-lock
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4317
OTEL_EXPORTER_OTLP_PROTOCOL=grpc
OTEL_SERVICE_NAME=vnext-execution-app-discovery
